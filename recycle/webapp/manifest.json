{
	"_version": "1.12.0",
	"sap.app": {
		"id": "opensap.recycle",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "opensap.recycle.view.Home",
			"type": "XML",
			"async": true,
			"id": "Home"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "opensap.recycle.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "opensap.recycle.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteHome",
					"pattern": "RouteHome",
					"target": [
						"TargetHome"
					]
				},
				{
					"name": "RouteRingFirstPage",
					"pattern": "RouteRingFirstPage",
					"target": [
						"RingFirstPage"
					]
				},
				{
					"name": "RouteRingSecondPage",
					"pattern": "RouteRingSecondPage",
					"target": [
						"RingSecondPage"
					]
				},
				{
					"name": "RouteLabelFirstPage",
					"pattern": "RouteLabelFirstPage",
					"target": [
						"LabelFirstPage"
					]
				},
				{
					"name": "RouteLabelSecondPage",
					"pattern": "RouteLabelSecondPage",
					"target": [
						"LabelSecondPage"
					]
				},
				{
					"name": "Succes",
					"pattern": "Succes",
					"target": [
						"SuccesView"
					]
				},
				{
					"name": "Error",
					"pattern": "Error",
					"target": [
						"ErrorView"
					]
				},
				{
					"name": "SendLocation",
					"pattern": "SendLocation",
					"target": [
						"SendLocation"
					]
				},
				{
					"name": "ItemBack",
					"pattern": "ItemBack",
					"target": [
						"ItemBack"
					]
				}
			],
			"targets": {
				"TargetHome": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Home",
					"viewName": "Home"
				},
				"RingSecondPage": {
					"viewType": "XML",
					"viewName": "RingSecondPage"
				},
				"RingFirstPage": {
					"viewType": "XML",
					"viewName": "RingFirstPage"
				},
				"LabelFirstPage": {
					"viewType": "XML",
					"viewName": "LabelFirstPage"
				},
				"LabelSecondPage": {
					"viewType": "XML",
					"viewName": "LabelSecondPage"
				},
				"SuccesView": {
					"viewType": "XML",
					"viewName": "SuccesView"
				},
				"ErrorView": {
					"viewType": "XML",
					"viewName": "Error"
				},
				"SendLocation": {
					"viewType": "XML",
					"viewName": "SendLocation"
				},
				"ItemBack": {
					"viewType": "XML",
					"viewName": "ItemBack"
				}
			}
		}
	}
}